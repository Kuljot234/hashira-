
const fs = require("fs");

// Read input.json (you can also take process.argv[2] if needed)
const input = JSON.parse(fs.readFileSync("input.json", "utf8"));

// Example: let's assume input.json has { "a": 1, "b": 3 }
const a = input.a;
const b = input.b;

// Print result in 1:3 format
console.log(`${a}:${b}`);
